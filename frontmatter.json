{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "description": "The text description for the article. It is used in the HTML metadata"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string",
          "description": "This is the Markdown string to be used as the summary for this article."
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Last Modified",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "single": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Article preview",
          "name": "preview",
          "type": "image",
          "isPreviewImage": true,
          "default": "feature.png"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "multiple": true,
          "choices": [
            "powershell",
            "chocolatey"
          ]
        },
        {
          "title": "Type",
          "name": "type",
          "type": "choice",
          "choices": [
            "default",
            "posts"
          ]
        }
      ]
    },
    {
      "name": "posts",
      "pageBundle": true,
      "previewPath": "posts/",
      "fileType": "md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "description": "The text description for the article. It is used in the HTML metadata"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string",
          "description": "This is the Markdown string to be used as the summary for this article."
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Last Modified",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "single": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Article preview",
          "name": "preview",
          "type": "image",
          "isPreviewImage": true,
          "default": "feature.png"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "list",
          "default": "{{dynamicKeywords}}"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "choice",
          "choices": [
            "default",
            "posts"
          ]
        }
      ]
    },
    {
      "name": "projects",
      "pageBundle": true,
      "previewPath": "projects/",
      "fileType": "md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Date Published",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string",
          "description": "This is the Markdown string to be used as the summary for this article."
        },
        {
          "title": "Show Reading Time",
          "name": "showReadingTime",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Last Modified",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "externalUrl",
          "name": "externalUrl",
          "type": "string"
        },
        {
          "title": "_build",
          "name": "_build",
          "type": "fields",
          "fields": [
            {
              "title": "render",
              "name": "render",
              "type": "string"
            },
            {
              "title": "list",
              "name": "list",
              "type": "string"
            }
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts",
      "previewPath": "posts/",
      "filePrefix": null
    },
    {
      "title": "assets",
      "path": "[[workspace]]/assets"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects",
      "previewPath": "projects/"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Active Directory",
    "Arduino",
    "Citrix",
    "Docker",
    "HomeAssistant",
    "IOT",
    "Linting",
    "MQTT",
    "OSX",
    "PowerShell"
  ],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.content.snippets": {
    "Call Out": {
      "description": "Create a call out box",
      "body": [
        "{{< alert \"twitter\" >}}",
        "Don't forget to [follow me](https://twitter.com/jpanther) on Twitter.",
        "{{< /alert >}}"
      ],
      "fields": []
    },
    "Figure": {
      "description": "",
      "body": [
        "{{< figure",
        "    src=\"[[&mediaUrl]]\"",
        "    alt=\"[[alt]]\"",
        "    caption=\"[[caption]]\"",
        "    >}}"
      ],
      "isMediaSnippet": true
    },
    "Icon": {
      "description": "",
      "body": "{{< icon \"[[icon]]\" >}}",
      "fields": [
        {
          "name": "icon",
          "type": "choice",
          "choices": [
            "amazon",
            "apple",
            "bars",
            "blogger",
            "bug",
            "check",
            "circle-info",
            "codepen",
            "comment",
            "dev",
            "dribbble",
            "edit",
            "email",
            "facebook",
            "flickr",
            "foursquare",
            "github",
            "gitlab",
            "google",
            "hashnode",
            "instagram",
            "keybase",
            "kickstarter",
            "lastfm",
            "lightbulb",
            "link",
            "linkedin",
            "list",
            "mastodon",
            "medium",
            "microsoft",
            "mobile",
            "moon",
            "orcid",
            "patreon",
            "pencil",
            "phone",
            "pinterest",
            "reddit",
            "researchgate",
            "search",
            "skull-crossbones",
            "slack",
            "snapchat",
            "soundcloud",
            "stack-overflow",
            "steam",
            "sun",
            "tag",
            "telegram",
            "tiktok",
            "triangle-exclamation",
            "tumblr",
            "twitch",
            "twitter",
            "whatsapp",
            "xmark",
            "youtube"
          ]
        }
      ]
    }
  },
  "frontMatter.content.placeholders": [
    {
      "id": "dynamicKeywords",
      "script": "./scripts/dynamicKeywords.py",
      "command": "python3.8"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.preview.host": "http://localhost:1313/"
}